{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server.js" }
    ],
    "redirects": [
      {
        "source": "^/(.*)$",
        "destination": "https://$1",
        "permanent": true
      }
    ]
  }