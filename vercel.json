{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server.js" }
    ],
    "redirects": [
      {
        "source": "http://consolecanvas.vercel.app/:path*",
        "destination": "https://consolecanvas.vercel.app/:path*",
        "permanent": true
      }
    ]
  }
  
  